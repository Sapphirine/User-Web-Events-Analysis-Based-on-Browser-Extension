{
  "manifest_version": 2,

  "name": "Web Wisp",
  "description": "This extension help you to visulize your web history and rescue your time.",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.jpeg",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "storage",
    "history"
  ]
}
